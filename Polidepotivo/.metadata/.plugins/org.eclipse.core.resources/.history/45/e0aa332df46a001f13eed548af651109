package modelos;

import java.time.LocalDate;

public class Horario_Club {
	private String diaSemana;
	private LocalDate horaApertura;
	private LocalDate horaCierre;
	public Horario_Club(String diaSemana, LocalDate horaApertura, LocalDate horaCierre) {
		super();
		this.diaSemana = diaSemana;
		this.horaApertura = horaApertura;
		this.horaCierre = horaCierre;
	}
	public String getDiaSemana() {
		return diaSemana;
	}
	public void setDiaSemana(String diaSemana) {
		this.diaSemana = diaSemana;
	}
	public LocalDate getHoraApertura() {
		return horaApertura;
	}
	public void setHoraApertura(LocalDate horaApertura) {
		this.horaApertura = horaApertura;
	}
	public LocalDate getHoraCierre() {
		return horaCierre;
	}
	public void setHoraCierre(LocalDate horaCierre) {
		this.horaCierre = horaCierre;
	}
	
}
